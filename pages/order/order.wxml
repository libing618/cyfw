<import src="../../../libs/weimport.wxml"/>
<import src="../../../libs/wedetail.wxml"/>
<view class="page">
  <view class="page__bd">
    <block wx:for="{{oArray}}" wx:key="{{index}}" wx:for-item="ac" wx:for-index="i">
      <view class="weui-cells">
        <view class="weui-cell weui-cell_access" hover-class="weui-cell_active" id="ac-{{i}}" bindtap="accheck">
          <template is="{{ achecked==i ? 'selecticon1' : 'selecticon2'}}" />
          <view class="weui-cell__bd">{{req.p}}</view>
          <view class="weui-cell__ft weui-cell__ft_in-access">{{ac}}</view>
        </view>
        <view hidden="{{achecked!=i}}">
          <block class="weui-cell" wx:for="{{mPage}}" wx:key="{{index}}" wx:for-item="id">
            <view wx:if="{{pageData[id][req.gname]==ac}}">
              <template is="i_{{req.t}}" data="{{p:pageData[id],e:pros[pageData[id].proObjectId],c:id,n:index}}"/>
            </view>
          </block>
        </view>
      </view>
    </block>
  </view>
</view>
